package com.arc.paddleRoyale.model{	import com.arc.paddleRoyale.vo.Agency;		import flash.events.Event;		public class AgencyEvent extends Event	{		public static const DEFAULT_NAME:String = "com.arc.paddleRoyale.model.AgencyEvent";				// Event types		public static const ADD:String 	= "add";		public static const KNOCKED_OUT:String 	= "knockedOut";				// Vars		public var agency:Agency;				public function AgencyEvent($type:String, $agency:Agency, $bubbles:Boolean=false, $cancelable:Boolean=false)		{			super($type, $bubbles, $cancelable);			agency = $agency;		}						/*--------------------------------------------------------------------------------------------------------		Helpers--------------------------------------------------------------------------------------------------------*/		public override function toString():String		{			return formatToString("AgencyEvent", "player", "type", "bubbles", "cancelable");		}	}}